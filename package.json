{
  "name": "react-native-deeplink-now",
  "version": "0.1.0",
  "description": "React Native / Expo wrapper for the DeepLink.now deferred deep linking SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "plugin",
    "app.plugin.js",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "build": "bob build",
    "release": "release-it",
    "postinstall": "pod-install"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "deep-linking"
  ],
  "repository": "https://github.com/jvgeee/react-native-deeplink-now",
  "author": "DeepLinkNow <support@deeplink.now>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jvgeee/react-native-deeplink-now/issues"
  },
  "homepage": "https://github.com/jvgeee/react-native-deeplink-now#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^17.0.21",
    "@types/react-native": "^0.70.0",
    "pod-install": "^0.1.0",
    "react": "17.0.2",
    "react-native": "0.70.0",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.2.2",
    "typescript": "^4.5.2"
  },
  "optionalDependencies": {
    "@expo/config-plugins": "^7.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
